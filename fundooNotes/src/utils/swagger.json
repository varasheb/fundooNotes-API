{
    "swagger": "2.0",
    "info": {
      "version": "V1",
      "title": " Google Keep ",
      "description": "This is the replica of google keep.User can register and login , User can do request for forgot password and he can reset password using that shared credentionals . User can loging and perform node CRUD."
    },
    "host": "localhost:3000",
    "basePath": "/api/v1/",
    "tags": [
      {
        "name": "Google keep using express CLI",
        "description": "This is the replica of google keep"
      }
    ],
    "schemes": ["http"],
    "consumes": ["application/json"],
    "produces": ["application/json"],
    "paths": {
      "/users": {
        "post": {
          "tags": ["User"],
          "description": "Register user...",
          "parameters": [
            {
              "name": "Register new name",
              "in": "body",
              "description": "Register user",
              "schema": {
                "$ref": "#/definitions/User"
              }
            }
          ],
          "produces": ["application/json"],
          "responses": {
            "201": {
              "description": "User created successfully"
            },
            "500": {
              "description": "Internal server error"
            },
            "400": {
                "description": "Bad Request"
              }
          }
        }
      },
      "/users/login": {
        "post": {
          "tags": ["User"],
          "description": "User login",
          "parameters": [
            {
              "name": "login user",
              "in": "body",
              "description": "User login",
              "schema": {
                "$ref": "#/definitions/login"
              }
            }
          ],
          "produces": ["application/json"],
          "responses": {
            "201": {
              "description": "sucessfully logged in"
            },
            "404": {
              "description": "email or password doesn't match"
            },
            "500": {
              "description": "Internal server error"
            },
            "400": {
                "description": "Bad Request"
              }
          }
        }
      },
      "/users/forgotpassword": {
        "post": {
          "tags": ["User"],
          "summary": "Forget password",
          "produces": [],
          "parameters": [
            {
              "name": "Forget Password",
              "in": "body",
              "description": "Forget Password",
              "required": false,
              "type": "string",
              "schema": {
                "$ref": "#/definitions/ForgetPassword"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "reset password link sent sucessfully"
            },
            "500": {
              "description": "could not send mail"
            },
            "400": {
                "description": "Bad Request"
              }
          }
        }
      },
      "/users/resetPassword": {
        "post": {
          "tags": ["User"],
          "summary": "reset password for User",
          "produces": [],
          "parameters": [
            {
              "name": "new password",
              "in": "body",
              "description": "new password",
              "required": true,
              "type": "string",
              "schema": {
                "$ref": "#/definitions/ResetPassword"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "reset Password is sucessfully done"
            },
            "500": {
              "description": "Internal server error"
            },
            "400": {
                "description": "Bad Request"
              }
          },
          "security": [
            {
              "JWT": []
            }
          ]
        }
      },
      "/notes": {
        "post": {
          "tags": ["Notes"],
          "summary": "Create a note for User",
          "produces": [],
          "parameters": [
            {
              "name": "create note",
              "in": "body",
              "description": "create note",
              "required": false,
              "type": "string",
              "schema": {
                "$ref": "#/definitions/Note"
              }
            }
          ],
          "responses": {
            "201": {
              "description": "note created succesfully"
            },
            "500": {
              "description": "Internal server error"
            },
            "400": {
              "description":"Bad Request"
            }
          },
          "security": [
            {
              "JWT": []
            }
          ]
        },
        "get": {
          "tags": ["Notes"],
          "summary": "Get all Notes",
          "produces": [],
          "parameters": [
            {
              "name": "Get Notes",

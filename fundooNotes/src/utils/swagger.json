            "500": {
              "description": "Internal server error"
            },
            "400": {
                "description":"Bad Request"
            }
          },
          "security": [
            {
              "JWT": []
            }
          ]
        },
        "delete": {
          "tags": ["Notes"],
          "summary": "Delete a note",
          "produces": [],
          "parameters": [
            {
              "name": "noteid",
              "in": "path",
              "required": true,
              "type": "string"
            },
            {
              "name": "delete a note",
              "description": "delete a note",
              "required": false
            }
          ],
          "responses": {
            "202": {
              "description": "Note deleted succesfully"
            },
            "500": {
              "description": "Internal server error"
            },
            "400": {
                "description":"Bad Request"
            }
          },
          "security": [
            {
              "JWT": []
            }
          ]
        }
      },
       "/notes/isarchive/{nodeid}": {
        "put": {
          "tags": ["Notes"],
          "summary": "Send a note to archive",
          "produces": [],
          "parameters": [
            {
              "name": "nodeid",
              "in": "path",
              "required": true,
              "type": "string"
            }
          ],
          "responses": {
            "202": {
              "description": "note sent to trash succesfully"
            },
            "500": {
              "description": "Internal server error"
            },
            "400": {
                "description":"Bad Request"
            }
          },
          "security": [
            {
              "JWT": []
            }
          ]
        }

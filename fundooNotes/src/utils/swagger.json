              "required": false
            }
          ],
          "responses": {
            "200": {
              "description": "notes fetched successfully"
            },
            "500": {
              "description": "Internal server error"
            },
            "400": {
                "description":"Bad Request"
            }
          },
          "security": [
            {
              "JWT": []
            }
          ]
        }
      },
      "/notes/{noteid}": {
        "get": {
          "tags": ["Notes"],
          "summary": "Get a single note by it's id",
          "produces": [],
          "parameters": [
            {
              "name": "noteid",
              "in": "path",
              "required": true,
              "type": "string"
            }
          ],
          "responses": {
            "200": {
              "description": "note fetched successfully"
            },
            "500": {
              "description": "Internal server error"
            },
            "400": {
                "description":"Bad Request"
            }
          },
          "security": [
            {
              "JWT": []
            }
          ]
        },
        "put": {
          "tags": ["Notes"],
          "summary": "Update a note for User",
          "produces": [],
          "parameters": [
            {
              "name": "noteid",
              "in": "path",
              "required": true,
              "type": "string"
            },
            {
              "name": "update note",
              "in": "body",
              "description": "update note",
              "required": false,
              "type": "string",
